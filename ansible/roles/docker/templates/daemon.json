{
    {% if docker_dns is defined %}
    "dns": [
        {% if docker_dns is string %}
        "{{docker_dns}}"
        {% else %}
        {% for dns in docker_dns %}
        "{{dns}}"{% if not loop.last %},{% endif %}
        {% endfor %}
        {% endif %}
    ],
    {% endif %}
    {% if docker_data_root is defined %}
    "data-root": "{{docker_data_root}}",
    {% endif %}
    "storage-driver": "{{docker_storage_driver}}"
}